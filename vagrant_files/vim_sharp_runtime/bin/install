cd ~/.vim_sharp_runtime

echo 'set runtimepath^=~/.vim_sharp_runtime
source ~/.vim_sharp_runtime/vimrc/basic.vim
source ~/.vim_sharp_runtime/vimrc/filetypes.vim
source ~/.vim_sharp_runtime/vimrc/plugins.vim
source ~/.vim_sharp_runtime/vimrc/extended.vim
try
source ~/.vim_sharp_runtime/custom_config.vim
catch
endtry' > ~/.vimrc.csharp

echo "Vim Sharp Installed.!"

